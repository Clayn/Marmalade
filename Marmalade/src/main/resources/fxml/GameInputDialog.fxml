<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="content" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.bplaced.clayn.marmalade.ui.dialog.GameInputDialogController">
   <left>
      <TreeView fx:id="tree" prefWidth="150.0" showRoot="false" BorderPane.alignment="CENTER" />
   </left>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <bottom>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button cancelButton="true" mnemonicParsing="false" onAction="#onCancel" text="%button.cancel" ButtonBar.buttonData="CANCEL_CLOSE" />
            <Button defaultButton="true" mnemonicParsing="false" onAction="#onSave" text="%button.save" ButtonBar.buttonData="YES" />
        </buttons>
      </ButtonBar>
   </bottom>
</BorderPane>
