<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.bplaced.clayn.marmalade.ui.controller.MainWindowController">
   <stylesheets>
        <URL value="@/styles/styles.css" />
    </stylesheets>
    <children>
      <fx:include source="component/MainMenu.fxml" />
      <BorderPane prefHeight="600.0" prefWidth="800.0">
         <center>
            <FlowPane fx:id="gamePane" hgap="10.0" vgap="10.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </BorderPane.margin></FlowPane>
         </center>
      </BorderPane>
   </children>
</VBox>
